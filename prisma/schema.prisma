
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  username  String
  password  String   // This will store the hashed password
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
